{
  "name": "express",
  "version": "1.0.0",
  "description": "This is an express app",
  "main": "src/index.js",
  "repository": "https://github.com/koechkevin/express.git",
  "author": "koechkevin",
  "license": "MIT",
  "private": false,
  "scripts": {
    "transpile": "babel src --out-dir dist -s",
    "clean": "rm -rf dist && rm -rf public",
    "build": "npm run clean && npm run transpile && webpack -p",
    "start": "node dist/index.js",
    "start:dev": "webpack -d && webpack-dev-server --mode development --open --hot",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "test": "NODE_ENV=test npm run migrate && jest --coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "chalk-console": "^1.1.0",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": "^5.8.8",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "nodemon": "^1.19.1"
  }
}
